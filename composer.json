{
  "name": "juhedata/cas",
  "description": "Adds CAS support to Laravel 6.9、7.x、8.x",
  "keywords": [
    "laravel support"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "juhedata",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6.9|^7.0|^8.0",
    "laravel/sanctum": "^2.8",
    "juhedata/laravel_cas_client": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "JuHeData\\CasLogin\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JuHeData\\CasLogin\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "JuHeData\\CasLogin\\CasLoginServiceProvider"
      ]
    }
  }
}
